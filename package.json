    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.53.1"
  },
  "bundleDependencies": [
    "eslint-config-prettier",
    "eslint-plugin-prettier",
    "prettier",
    "typescript-eslint"
